<!--pages/registerForm/registerForm.wxml-->
<form class="page centerPage-wrap">
  <view class="centerPage-user-info">
     <view class="centerPage-info-item">
      <label></label>
      <input id="Name" class="nickname" bindinput="inputName" placeholder="姓氏" value="{{name}}"></input>
    </view> 
    <view class="centerPage-info-item">
      <label></label>
      <input id="emailOrPhone"  bindinput="inputPhone" value="{{verifyPhone}}" class="nickname" placeholder="电邮地址/电话号码"></input>
    </view> 
    <view class="centerPage-info-item">
      <label></label>
      <input id="Country" bindinput="inputCountry" value="{{country}}" class="nickname" placeholder="国家"></input>
    </view> 
    <view class="centerPage-info-item">
      <label></label>
      <input id="Brand" bindinput="inputBrand" value="{{brand}}" class="nickname" placeholder="品牌"></input>
    </view> 
    <view class="centerPage-info-item">
      <label></label>
      <input id="Store" bindinput="inputStore" value="{{store}}" class="nickname" placeholder="门店"></input>
    </view> 
    <view class="centerPage-info-item">
      <label></label>
      <input id="Channel" bindinput="inputChannel" value="{{channel}}" class="nickname" placeholder="柜台"></input>
    </view> 
  </view>
  <view class="centerPage-save-btn" bindtap="createCard">创建 
</view>
</form>

